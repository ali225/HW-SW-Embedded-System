#ifndef DIO_H_
#define DIO_H_


typedef enum {
	PORTA,
	PORTB,
	PORTC,
	PORTD
}

/******************* Function declaration section ***************/

extern stdReturn_t DIO_init(portX_t PortName,pinX_t PinNume,pinDir_t direction);
extern stdReturn_t DIO_read(portX_t PortName,pinX_t PinNume , pinState_t *ppinStat);
extern stdReturn_t DIO_write(portX_t PortName,pinX_t PinNume, pinState_t pinStat);


#endif /* DIO_H_ */
