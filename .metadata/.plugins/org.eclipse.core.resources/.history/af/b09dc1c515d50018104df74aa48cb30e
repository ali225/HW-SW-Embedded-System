 *      Author: Eng Ali Gamal
 */


int main(void)
{

	LCD_VidInit();
	Keypad_vidInit();



	while(1)
	{
		for(u8 i=0; i<16; i++)
		{
			if(Keypad_u8GetButtonStatus(i)==1)
			{
				LCD_VidSetPosition(0,0);
				LCD_VidWriteNumber(i);
			}
		}
	}

	return -1;
}
