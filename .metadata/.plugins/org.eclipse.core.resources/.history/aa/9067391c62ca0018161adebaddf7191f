#define F_CPU 12000000UL
#include <avr/io.h>
#include <util/delay.h>

int main(void)
{
	DDRD = 0b00000011;

	while(1)
	{
		PORTD =	0b00000011;
		_delay_ms(100);
		PORTD = 0b00000000;
		_delay_ms(100);
	}
}

