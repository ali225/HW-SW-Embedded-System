#include <avr/delay.h>
#include "Types.h"
#include "Macros.h"
#include "REGISTERS.h"
#include "DIO.h"
#define F_CPU 12000000UL


int main(void)
{
	DIO_VidSetPortDirection(3,255);
	DIO_VidSetPortValue(3,255);

	while(1)
	{
		PORTD = 0b10000001;
		_delay_ms(100);
		PORTD = 0b11100011;
		_delay_ms(200);
		PORTD = 0b00000000;
		_delay_ms(300);
		PORTD = 0b11100011;
		_delay_ms(222);

	}

	return 0;
}
