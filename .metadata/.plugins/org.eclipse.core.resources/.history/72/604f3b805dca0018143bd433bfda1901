#include <util/delay.h>

#include "DIO.h"

#define F_CPU 12000000UL


int main(void)
{

	DIO_VidSetPinDirection(3,1,1);
	DIO_VidSetPinDirection(3,2,1);

	while(1)
		{


		//	DIO_VidSetPinValue(3,1,1);

			DIO_VidSetPinValue(3,1,0);
			_delay_ms(5000);
			DIO_VidSetPinValue(3,1,1);
			_delay_ms(5000);
			DIO_VidSetPinValue(3,2,0);
			_delay_ms(100);
			DIO_VidSetPinValue(3,2,1);
			_delay_ms(100);

		}


	return 0;
}
