#define F_CPU 12000000UL
#include <avr/io.h>
#include <util/delay.h>

int main(void)
{
	DDRD |= 1<<PD0; // set PB6 to output

	while (1)
	{
		PORTD &= ~(1<<PD0); // LED on
		_delay_ms(100);
		PORTD |= 1<<PD0; // LED off
		_delay_ms(100);
	}
	return 0;
}
