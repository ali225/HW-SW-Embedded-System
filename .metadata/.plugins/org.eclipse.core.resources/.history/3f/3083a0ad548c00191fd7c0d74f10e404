#include <stdio.h>
#include <string.h>
#include <stdlib.h>

void vidRevers(int array_size , int* array);

int main()
{

}

void vidRevers(int array_size , int* array)
{
	int i =0 , temp;

	while(i < (array_size / 2))
	{
		temp = array[i];
		array[i] = array [array_size - 1 - i];
		array [array_size - 1 - i] = temp;
		i++;
	}

}
