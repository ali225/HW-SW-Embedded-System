#ifndef TYPES_H_
#define TYPES_H_

	typedef unsigned char u8;
	typedef unsigned short int u16;
	typedef unsigned int u32;
	typedef unsigned long long int u64;
	typedef signed char s8;
	typedef signed short int s16;
	typedef signed int s32;
	typedef float f32;
	typedef double f64;


#endif /* TYPES_H_ */
