#define F_CPU 16000000UL //16 MHZ
#include "avr/io.h"
#include "avr/delay.h"
#include "util/delay.h"

int main() {
	DDRD = 0xff;
	PORTD= 0xff;

	while (1) {

		PD7 = 0xff;
		_delay_ms(500);
		PD7 = 0x00;
		_delay_ms(500);
	}

}
